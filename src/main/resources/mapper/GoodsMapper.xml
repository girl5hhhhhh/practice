<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.practice.mapper.GoodsMapper">

    <!--获取商品列表-->
    <select id="findGoodsVo" resultType="com.example.practice.pojo.shuJuKu.Goods">
        SELECT
            g.id,
            g.goods_name,
            g.goods_title,
            g.goods_img,
            g.goods_detail,
            g.goods_price,
            g.goods_stock
        FROM
            t_goods g
    </select>

</mapper>
